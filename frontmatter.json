{
  "draft": true,
  "frontMatter.framework.id": "11ty",
  "frontMatter.preview.host": "http://localhost:8080",
  "frontMatter.content.pageFolders": [
    {
      "title": "men",
      "path": "[[workspace]]/src/men"
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "alt",
          "name": "alt",
          "type": "string"
        }
      ]
    }
  ]
}
